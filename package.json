{
	"name": "@adacon-norway/adacon",
	"version": "0.0.0-development",
	"description": "AdaCon Norway in Oslo",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"start": "vite",
		"build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adacon-norway/adacon.git"
	},
	"keywords": ["adacon-norway", "norway"],
	"author": "Tech Women Norway",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/adacon-norway/adacon/issues"
	},
	"homepage": "https://github.com/adacon-norway/adacon#readme",
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@preact/preset-vite": "2.9.0",
		"@types/node": "22.5.4",
		"husky": "9.1.5",
		"lint-staged": "15.2.10",
		"lucide": "0.439.0",
		"preact-render-to-string": "6.5.10",
		"prettier": "3.3.3",
		"rehype-format": "5.0.0",
		"rehype-stringify": "10.0.0",
		"remark": "15.0.1",
		"remark-extract-frontmatter": "3.2.0",
		"remark-frontmatter": "5.0.0",
		"remark-rehype": "11.1.0",
		"typescript": "5.6.2",
		"vike": "0.4.195",
		"vite": "5.4.3",
		"yaml": "2.5.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{md,yaml,yml}": ["prettier --write"],
		"*.{json,ts,tsx}": [
			"npx @biomejs/biome format --write",
			"npx @biomejs/biome lint --apply"
		]
	},
	"engines": {
		"node": ">=20",
		"npm": ">=9"
	},
	"release": {
		"branches": ["saga"],
		"remoteTags": true,
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/github",
				{
					"successComment": false,
					"failTitle": false
				}
			]
		]
	},
	"dependencies": {
		"bootstrap": "5.3.3",
		"classnames": "2.5.1",
		"date-fns": "3.6.0",
		"lucide-preact": "0.439.0",
		"preact-intersection-observer": "2.3.6",
		"remark-gfm": "4.0.0",
		"styled-components": "6.1.13"
	}
}
