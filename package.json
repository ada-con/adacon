{
	"name": "@ada-con/adacon",
	"version": "0.0.0-development",
	"description": "Ada Con Norway 2023 in Oslo",
	"scripts": {
		"prepare": "husky install",
		"start": "vite",
		"build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ada-con/adacon.git"
	},
	"keywords": ["ada-con", "norway"],
	"author": "Tech Women Norway",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ada-con/adacon/issues"
	},
	"homepage": "https://github.com/ada-con/adacon#readme",
	"devDependencies": {
		"@preact/preset-vite": "2.5.0",
		"@types/node": "20.5.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"husky": "8.0.3",
		"lint-staged": "14.0.0",
		"preact-render-to-string": "6.2.1",
		"rehype-format": "4.0.1",
		"rehype-stringify": "9.0.4",
		"remark": "14.0.3",
		"remark-extract-frontmatter": "3.2.0",
		"remark-frontmatter": "4.0.1",
		"remark-rehype": "10.1.0",
		"rome": "12.1.3-nightly.4c8cf32",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"vite-plugin-ssr": "0.4.136",
		"yaml": "2.3.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,md,json,yaml,yml,html}": [
			"npx rome format --write",
			"npx rome lint"
		]
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"release": {
		"branches": ["saga"],
		"remoteTags": true,
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/github"
		]
	},
	"dependencies": {
		"bootstrap": "5.3.1",
		"date-fns": "2.30.0",
		"lucide": "0.268.0",
		"lucide-preact": "0.268.0",
		"preact-intersection-observer": "2.3.6",
		"styled-components": "6.0.0-beta.11"
	},
	"prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
