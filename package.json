{
  "name": "@ada-con/adacon",
  "version": "0.0.0-development",
  "description": "Ada Con Norway 2023 in Oslo",
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ada-con/adacon.git"
  },
  "keywords": [
    "ada-con",
    "norway"
  ],
  "author": "Tech Women Norway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ada-con/adacon/issues"
  },
  "homepage": "https://github.com/ada-con/adacon#readme",
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.6",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "12.0.2",
    "@preact/preset-vite": "2.5.0",
    "@types/node": "18.15.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "preact-render-to-string": "5.2.6",
    "prettier": "2.8.5",
    "rehype-format": "4.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.2",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "4.0.1",
    "remark-rehype": "10.1.0",
    "typescript": "5.0.2",
    "vite": "4.2.3",
    "vite-plugin-ssr": "0.4.99",
    "yaml": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "bootstrap": "5.2.3",
    "date-fns": "2.29.3",
    "lucide": "0.127.0",
    "lucide-preact": "0.127.0",
    "preact-intersection-observer": "2.3.6",
    "styled-components": "6.0.0-beta.11"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
